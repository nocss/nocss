// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "rules": {
    "prefer-template": "off",
    "no-param-reassign": "off",
    "object-curly-spacing": "off",
    "quote-props": "off",
    "max-len": ["error", 80]
  }
}
